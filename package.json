{
  "name": "@mayahq/electron-log-loggly",
  "version": "1.0.2",
  "description": "Just a very simple logging module for your Electron application forked for working with Loggly remote URL",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint '**/*.js' --ignore-pattern '**/dist/*.js' && tsc --noEmit",
    "test": "humile 'src/**/*spec.js'",
    "test:full": "npm run test && npm run lint"
  },
  "typings": "src/index.d.ts",
  "repository": "mayahq/electron-log-loggly",
  "files": [
    "src/*",
    "!__specs__"
  ],
  "keywords": [
    "electron",
    "atom",
    "log",
    "logger",
    "logging",
    "windows",
    "mac",
    "osx",
    "linux",
    "desktop"
  ],
  "author": "Shubham Mishra",
  "license": "MIT",
  "bugs": "https://github.com/mayahq/electron-log-loggly/issues",
  "homepage": "https://github.com/megahertz/electron-log-loggly#readme",
  "devDependencies": {
    "@types/node": "^13.1.8",
    "electron": "*",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "humile": "*",
    "typescript": "*",
    "webpack": "*",
    "webpack-cli": "*"
  }
}
